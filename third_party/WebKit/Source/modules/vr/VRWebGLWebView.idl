[
    DoNotCheckConstants,
    Constructor
] interface VRWebGLWebView {

    const long HAVE_NOTHING = 0;
    const long HAVE_METADATA = 1;
    const long HAVE_CURRENT_DATA = 2;
    const long HAVE_FUTURE_DATA = 3;
    const long HAVE_ENOUGH_DATA = 4;

    const long readyState = 2;

    attribute DOMString src;
    readonly attribute long id;

    readonly attribute long width;
    readonly attribute long height;

    void touchstart(float x, float y);
    void touchmove(float x, float y);
    void touchend(float x, float y);

    void back();
    void forward();
    void reload();
    void voiceSearch();

    void keydown(long keycode);
    void keyup(long keycode);

    void cursorenter(float x, float y);
    void cursormove(float x, float y);
    void cursorexit();

    attribute boolean transparent;

    void setScale(long scale, boolean loadWithOverviewMode, boolean useWideViewPort);
};
