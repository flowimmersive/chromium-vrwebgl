[
    DoNotCheckConstants,
    Constructor
] interface VRWebGLVideo {

    const long HAVE_NOTHING = 0;
    const long HAVE_METADATA = 1;
    const long HAVE_CURRENT_DATA = 2;
    const long HAVE_FUTURE_DATA = 3;
    const long HAVE_ENOUGH_DATA = 4;

    readonly attribute long id;

    attribute DOMString src;
    attribute double volume;
    attribute boolean loop;
    attribute double currentTime;
    attribute long readyState;

    readonly attribute boolean paused;
    readonly attribute double duration;
    readonly attribute long videoWidth;
    readonly attribute long videoHeight;
    attribute boolean ended;

    void play();
    void pause();
};
